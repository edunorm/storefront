{
  "name": "@edunorm/woocommerce-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Fetch-based, Cloudflare Workers-compatible WooCommerce REST API client (type-only imports from @woocommerce/woocommerce-rest-api).",
  "keywords": [
    "woocommerce",
    "cloudflare-workers",
    "fetch",
    "rest",
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check-types": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "@edunorm/typescript-config": "workspace:*"
  }
}

